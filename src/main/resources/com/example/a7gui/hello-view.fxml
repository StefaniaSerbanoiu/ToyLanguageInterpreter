<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="BOTTOM_RIGHT" prefHeight="542.0" prefWidth="578.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.a7gui.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label prefHeight="18.0" prefWidth="616.0" text="Select one of the programs below:" />
   <ListView onEditStart="#onHelloButtonClick" prefHeight="527.0" prefWidth="538.0" />
   <ButtonBar prefHeight="40.0" prefWidth="200.0" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Hello!" textFill="#6925db" />
</VBox>
